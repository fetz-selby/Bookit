<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.error-font-style{color:#D14836;}
	</ui:style>
	<g:HTMLPanel>
	    <section class="panel panel-default confirmpop-style">
			<header class="panel-heading confirmpop-header">
				<span class="h4 confirmpop-header-txt" ui:field="formTitle">Login ...</span>
				<span class="pull-right font-bold form-close-icon" style="color: #FFFFFF;" ui:field="closeBtn">x</span>
			</header>
			<div class="panel-body">
				<span class="{style.error-font-style} gwt-hide" ui:field="message">wrong username or password</span>
				
				<div class="form-group pull-in clearfix">
					<div class="col-sm-8">
						<g:TextBox styleName="form-control parsley-validated" ui:field="fieldNameBox"/>
							<ul id="parsley-9505912570748478" class="parsley-error-list"
								style="display: block;">
								<li class="required gwt-hide {style.error-font-style}" ui:field="fieldNameError">Invalid Email</li>
							</ul>
					</div>
					
					<div class="col-sm-8">
						<g:PasswordTextBox styleName="form-control parsley-validated" ui:field="passwordFieldBox"/>
							<ul id="parsley-9505912570748478" class="parsley-error-list"
								style="display: block;">
								<li class="required gwt-hide" ui:field="passwordFieldrror"></li>
							</ul>
					</div>
					
				</div>
				
				
			</div>
			
			<footer class="panel-footer text-right bg-light lter">
			    <g:Anchor href="javascript:void(0)" styleName="h4 text-success confirmpop-cancel-link" ui:field="cancelLink" />
				<g:Button styleName="btn btn-primary btn-s-xs confirmpop-button" ui:field="confirmBtn">Confirm</g:Button>
			</footer>
		</section>
	</g:HTMLPanel>
</ui:UiBinder> 