<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.widget-float-style{float: left;}
		.widget-float-style2{float: right;}
		.widget-font-style{color: #009945;}
	</ui:style>
	<g:HTMLPanel styleName="m-l m-b col-md-5 user-sortable-login-field">
		<label class="hidden-xs m-l-sm user-sortable-form-label" ui:field="label"></label>
		<div class="clearfix"></div>
		<div class="{style.widget-float-style}">
		    <label class="hidden-xs m-l-sm user-sortable-form-label {style.widget-font-style}" ui:field="labelLogin">Unknown</label>
		</div>
		
		<div class="{style.widget-float-style2}">
		    <g:Button styleName="btn btn-primary btn-s-xs {style.widget-float-style2}" ui:field="loginBtn">Login</g:Button>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 